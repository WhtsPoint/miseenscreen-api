<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Model\CallForm">
        <id name="id" column="id" />
        <field type="text" name="comment" column="comment" />
        <field type="text" name="fullName" column="fullName" />
        <field type="text" name="companyName" column="companyName" />
        <field type="text" name="employeeNumber" column="employeeNumber" />
        <field type="text" name="phone" column="phone" />
        <field type="text" name="email" column="email" />
        <field name="files" type="json" column="files" />
        <field name="postedAt" type="datetime_immutable" nullable="true" />
        <field name="services" type="services" column="services" nullable="true" />
        <field name="status" type="formStatus" nullable="true" />
    </entity>
</doctrine-mapping>