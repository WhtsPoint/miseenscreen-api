<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Model\Subscription">
        <id name="id" column="id" />
        <field name="email" type="email" unique="true" column="email" />
        <field name="postedAt" type="datetime_immutable" nullable="true" />
    </entity>
</doctrine-mapping>